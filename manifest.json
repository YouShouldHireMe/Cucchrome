{
  "name": "Cuccrhome",
  "version": "1.0",
  "manifest_version": 2,
  "description": "An extension honoring the Legend of Zelda series by spawning annoying cuccos",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/*", "http://www.google.com/*"],
      "js":  ["myscript.js"]
    }
  ]
}
